  "devDependencies": {
    "jsdoc-to-markdown": "^6.0.1",
    "mocha": "^8.1.3",
    "pre-commit": "^1.2.2",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@types/node": "^14.11.8"
  }
}
